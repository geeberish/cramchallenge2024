import nvdlib

def main():

  r = nvdlib.searchCPE(keywordSearch = 'Microsoft Exchange', limit = 2)
  for eachCPE in r:
      print(eachCPE.cpeName)


  #'cpe:2.3:a:ca:unicenter_management_microsoft_exchange:-:*:*:*:*:*:*:*'
  #'cpe:2.3:a:microsoft:exchange_instant_messenger:-:*:*:*:*:*:*:*''

if __name__ == '__main__':
  main()